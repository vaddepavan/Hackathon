			function validate(){

				var pattern1 = /^[A-Za-z0-9]{1,}[~@#\^\$&\*\(\)-_\+=\[\]\{\}\|\\,\.\?\s]*\w{1,}$/;
				var pattern2 = /^[A-Za-z0-9]{3,}[~@#\^\$&\*\(\)-_\+=\[\]\{\}\|\\,\.\?\s]+\w{2,}$/;
				var email=document.form1.t1.value;
				var password=document.form1.t2.value;
				if(!(email.search(pattern1)==-1)&&!(password.search(pattern2)==-1)){
					return true;
				}
				else{
					if(email.search(pattern1)==-1)
					alert("enter valid email address");
					if(password.search(pattern2)==-1)
					alert("enter valid password");
					return false;
				}
		 
			}
		</script>
